<%= form_with model: [ category, task ] do |form| %>
    <p>
        <%= form.label :name %><br>
        <%= form.text_field :name %>
    </p>
    <p>
        <%= form.label :notes %><br>
        <%= form.text_field :notes %>
    </p>
    <p>
        <%= form.label :due_date %><br>
        <%= form.datetime_local_field :due_date %>
    </p>
    <p>
        <%= form.label :complete %><br>
        <%= form.check_box :complete %>
    </p>
    <p>
        <%= form.submit %><br>
    </p>
<% end %>
